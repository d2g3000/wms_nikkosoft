@model IEnumerable<SaArtUbicacion>

@if (Model.Count()>0)
{
    <table class="table table-striped p-4 overflow-scroll" style="background-color:lightgray;">
        <thead class="text-white" style="background-color:#746ab0;">
            <tr>
                <th>código</th>
                <th>Cantidad</th>
                <th>Almacén</th>
                <th>Ubicación</th>
                <th>Documento</th>
                <th>Stock actual</th>
            </tr>
        </thead>
   
    <tbody>

   
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.co_art
            </td>
            <td>
                @item.cantidad
            </td>
            <td>
                @item.co_alma
            </td>
            <td>
                @item.ubicacion
            </td>
            <td>@item.tipo_doc</td>
            <td>@item.stock_actual</td>
        </tr>
     
    }
    </tbody>
    </table>
}else
{
    <h1>No hay ubicaciones registradas</h1>
}
